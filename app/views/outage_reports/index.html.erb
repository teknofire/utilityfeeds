<p id="notice"><%= notice %></p>

<h1>Listing Outage Reports</h1>

<table>
  <thead>
    <tr>
      <th>Service</th>
      <th>Provider</th>
      <th>Location</th>
      <th>Address</th>
      <th>Notes</th>
      <th>Started at</th>
      <th>Ended at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outage_reports.each do |outage_report| %>
      <tr>
        <td><%= outage_report.service %></td>
        <td><%= outage_report.provider %></td>
        <td><%= outage_report.location %></td>
        <td><%= outage_report.address %></td>
        <td><%= outage_report.notes %></td>
        <td><%= outage_report.started_at %></td>
        <td><%= outage_report.ended_at %></td>
        <td><%= link_to 'Show', outage_report %></td>
        <td><%= link_to 'Edit', edit_outage_report_path(outage_report) %></td>
        <td><%= link_to 'Destroy', outage_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outage report', new_outage_report_path %>
